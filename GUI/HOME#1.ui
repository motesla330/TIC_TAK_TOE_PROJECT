        background: transparent;
    }
    
    QMenuBar::item:selected {
        background: rgba(255,255,255,0.1);
        border-radius: 4px;
    }
    
    /* Status Bar */
    QStatusBar {
        background: rgba(0,0,0,0.2);
        color: white;
        font-family: 'Segoe UI', Arial, sans-serif;
        border: none;
    }
